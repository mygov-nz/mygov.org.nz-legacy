{
    "manifest_version": 2,
    "name": "MyGov",
    "short_name": "MyGov",
    "description": "MyGov.org.nz",
    "default_locale": "en_NZ",
    "start_url": "/",
    "display": "standalone",
    "offline_enabled": true,
    "background_color": "#fff",
    "theme_color": "#f89828",
    "icons": [
        {
            "src": "/images/launcher-icon.png",
            "sizes": "48x48",
            "type": "image/png"
        },
        {
            "src": "/images/launcher-icon@2x.png",
            "sizes": "96x96",
            "type": "image/png"
        },
        {
            "src": "/images/launcher-icon@3x.png",
            "sizes": "144x144",
            "type": "image/png"
        },
        {
            "src": "/images/launcher-icon@4x.png",
            "sizes": "192x192",
            "type": "image/png"
        }
    ]
}

